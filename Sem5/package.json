  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "bootstrap": "^4.3.1",
    "node-sass-chokidar": "^1.3.5",
    "react": "^16.10.2",
    "react-bodymovin": "^2.0.0",
    "react-dom": "^16.10.2",
    "react-hook-form": "^3.26.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-toast-notifications": "^2.2.5",
    "reactstrap": "^8.1.0"
  },
  "scripts": {
    "start": "npm run watch:sass | react-scripts start",
    "build:sass": "node-sass-chokidar src/styles/ -o src/styles/",
    "watch:sass": "npm run build:sass && node-sass-chokidar src/styles/ -o src/styles/ --watch --recursive --include-path",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
